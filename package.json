{
  "name": "treege-consumer",
  "description": "A React library to consume easily form generated from Tree",
  "version": "1.21.1",
  "type": "module",
  "types": "dist/main.d.ts",
  "main": "dist/main.umd.cjs",
  "module": "dist/main.js",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:Tracktor/treege-consumer.git",
  "author": "MickaÃ«l Austoni",
  "bugs": {
    "url": "https://github.com/Tracktor/treege-consumer/issues"
  },
  "homepage": "https://github.com/Tracktor/treege-consumer#readme",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --max-warnings=0 && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "prepare": "yarn run build && husky install",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@tanstack/react-query": "^5.17.15",
    "@tracktor/design-system": "^1.48.3",
    "@tracktor/react-utils": "^1.11.0",
    "autosuggest-highlight": "^3.3.4",
    "commit-and-tag-version": "^12.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/google.maps": "^3.54.3",
    "@types/lodash-es": "^4.17.9",
    "@types/react": "^18.2.26",
    "@types/react-dom": "^18.2.11",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint-config-react-tracktor": "^1.7.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "commit-and-tag-version": {
    "releaseCommitMessageFormat": "chore(release): {{currentTag}} [skip ci]",
    "tagPrefix": ""
  },
  "peerDependencies": {
    "@tracktor/design-system": ">=1",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "react",
    "treege",
    "treege consumer"
  ]
}
